				<peatykkNr>18</peatykkNr>
				<kuvatavNr><![CDATA[18. peatükk]]></kuvatavNr>
				<peatykkPealkiri>MENETLUSKULUD</peatykkPealkiri>
				<jagu id="jg1">
					<jaguNr>1</jaguNr>
					<kuvatavNr><![CDATA[1. jagu]]></kuvatavNr>
					<jaguPealkiri>Üldsätted</jaguPealkiri>
					<paragrahv id="para138">
						<paragrahvNr>138</paragrahvNr>
						<kuvatavNr><![CDATA[§ 138. ]]></kuvatavNr>
						<paragrahvPealkiri>Menetluskulude koosseis ja arvestus</paragrahvPealkiri>
						<loige id="para138lg1">
							<loigeNr>1</loigeNr>
							<kuvatavNr><![CDATA[(1)]]></kuvatavNr>
							<sisuTekst>
								<tavatekst>Menetluskulud on menetlusosaliste kohtukulud ja kohtuvälised kulud.</tavatekst>
							</sisuTekst>
						</loige>
						<loige id="para138lg2">
							<loigeNr>2</loigeNr>
							<kuvatavNr><![CDATA[(2)]]></kuvatavNr>
							<sisuTekst>
								<tavatekst>Kohtukulud on riigilõiv ja asja läbivaatamise kulud.</tavatekst>
							</sisuTekst>
							<muutmismarge>
								<avaldamismarge>
									<RTosa>RT I</RTosa>
									<avaldamineKuupaev>2021-12-08</avaldamineKuupaev>
									<RTartikkel>1</RTartikkel>
									<aktViide>108122021001</aktViide>
								</avaldamismarge>
								<joustumine>2022-01-01</joustumine>
							</muutmismarge>
						</loige>
						<loige id="para138lg3">
							<loigeNr>3</loigeNr>
							<kuvatavNr><![CDATA[(3)]]></kuvatavNr>
							<sisuTekst>
								<tavatekst>Igas kohtuastmes peab kohus arvestust asjas kantud menetluskulude, muu hulgas asja läbivaatamise kulude kohta.</tavatekst>
							</sisuTekst>
						</loige>
					</paragrahv>
					<paragrahv id="para139">
						<paragrahvNr>139</paragrahvNr>
						<kuvatavNr><![CDATA[§ 139. ]]></kuvatavNr>
						<paragrahvPealkiri>Riigilõiv</paragrahvPealkiri>
						<loige id="para139lg1">
							<loigeNr>1</loigeNr>
							<kuvatavNr><![CDATA[(1)]]></kuvatavNr>
							<sisuTekst>
								<tavatekst>Riigilõiv on menetlustoimingu tegemise eest seaduse kohaselt Eesti Vabariigile tasutav rahasumma.</tavatekst>
							</sisuTekst>
						</loige>
						<loige id="para139lg2">
							<loigeNr>2</loigeNr>
							<kuvatavNr><![CDATA[(2)]]></kuvatavNr>
							<sisuTekst>
								<tavatekst>Riigilõivu tuleb tasuda menetlustoimingult, mille tegemise eest on riigilõivuseaduses sätestatud riigilõiv.</tavatekst>
							</sisuTekst>
							<muutmismarge>
								<avaldamismarge>
									<RTosa>RT I</RTosa>
									<avaldamineKuupaev>2012-06-29</avaldamineKuupaev>
									<RTartikkel>3</RTartikkel>
									<aktViide>129062012003</aktViide>
								</avaldamismarge>
								<joustumine>2012-07-01</joustumine>
							</muutmismarge>
						</loige>
						<loige id="para139lg2b1">
							<loigeNr ylaIndeks="1">2</loigeNr>
							<kuvatavNr><![CDATA[(2<sup>1</sup>)]]></kuvatavNr>
							<muutmismarge>
								<tavatekst>Kehtetu - </tavatekst>
								<avaldamismarge>
									<RTosa>RT I</RTosa>
									<avaldamineKuupaev>2014-06-21</avaldamineKuupaev>
									<RTartikkel>8</RTartikkel>
									<aktViide>121062014008</aktViide>
								</avaldamismarge>
								<joustumine>2014-07-01</joustumine>
							</muutmismarge>
						</loige>
						<loige id="para139lg3">
							<loigeNr>3</loigeNr>
							<kuvatavNr><![CDATA[(3)]]></kuvatavNr>
							<muutmismarge>
								<tavatekst>Kehtetu - </tavatekst>
								<avaldamismarge>
									<RTosa>RT I</RTosa>
									<avaldamineKuupaev>2012-06-29</avaldamineKuupaev>
									<RTartikkel>3</RTartikkel>
									<aktViide>129062012003</aktViide>
								</avaldamismarge>
								<joustumine>2012-07-01</joustumine>
							</muutmismarge>
						</loige>
						<loige id="para139lg4">
							<loigeNr>4</loigeNr>
							<kuvatavNr><![CDATA[(4)]]></kuvatavNr>
							<muutmismarge>
								<tavatekst>Kehtetu - </tavatekst>
								<avaldamismarge>
									<RTosa>RT I</RTosa>
									<avaldamineKuupaev>2012-06-29</avaldamineKuupaev>
									<RTartikkel>3</RTartikkel>
									<aktViide>129062012003</aktViide>
								</avaldamismarge>
								<joustumine>2012-07-01</joustumine>
							</muutmismarge>
						</loige>
						<loige id="para139lg5">
							<loigeNr>5</loigeNr>
							<kuvatavNr><![CDATA[(5)]]></kuvatavNr>
							<sisuTekst>
								<tavatekst>Riigilõivu ei tasuta menetluse algatamise avalduselt hagita asjas, mille kohus võib lahendada omal algatusel. See ei välista riigilõivu tasumist kohtulahendi alusel.</tavatekst>
							</sisuTekst>
						</loige>
						<loige id="para139lg6">
							<loigeNr>6</loigeNr>
							<kuvatavNr><![CDATA[(6)]]></kuvatavNr>
							<sisuTekst>
								<tavatekst>Riigilõivu ei tasuta menetlusabi taotluselt.</tavatekst>
							</sisuTekst>
						</loige>
					</paragrahv>
					<paragrahv id="para140">
						<paragrahvNr>140</paragrahvNr>
