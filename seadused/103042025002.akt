						</sisuTekst>
						<alampunkt id="para475lg1p1">
							<alampunktNr>1</alampunktNr>
							<kuvatavNr><![CDATA[1) ]]></kuvatavNr>
							<sisuTekst>
								<tavatekst>maksekäsu kiirmenetlus;</tavatekst>
							</sisuTekst>
						</alampunkt>
						<alampunkt id="para475lg1p1b1">
							<alampunktNr ylaIndeks="1">1</alampunktNr>
							<kuvatavNr><![CDATA[1<sup>1</sup>) ]]></kuvatavNr>
							<sisuTekst>
								<tavatekst>kollektiivse esindushagi asjad;</tavatekst>
							</sisuTekst>
							<muutmismarge>
								<avaldamismarge>
									<RTosa>RT I</RTosa>
									<avaldamineKuupaev>2024-12-31</avaldamineKuupaev>
