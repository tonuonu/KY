							</muutmismarge>
						</paragrahv>
					</jaotis>
					<jaotis id="jaotis6">
						<jaotisNr>6</jaotisNr>
						<kuvatavNr><![CDATA[6. jaotis]]></kuvatavNr>
						<jaotisPealkiri>Igamine</jaotisPealkiri>
						<paragrahv id="para110">
							<paragrahvNr>110</paragrahvNr>
							<kuvatavNr><![CDATA[§ 110.]]></kuvatavNr>
							<paragrahvPealkiri>Igamise sisu</paragrahvPealkiri>
							<loige id="para110lg1">
								<loigeNr>1</loigeNr>
								<kuvatavNr><![CDATA[(1)]]></kuvatavNr>
								<sisuTekst>
									<tavatekst>Vallasomand tekib igamisega, kui isik valdab vallasasja katkematult viie aasta jooksul nagu omanik.</tavatekst>
								</sisuTekst>
								<muutmismarge>
									<avaldamismarge>
										<RTosa>RT I</RTosa>
										<RTaasta>2003</RTaasta>
										<RTnr>13</RTnr>
										<RTartikkel>64</RTartikkel>
										<aktViide>252549</aktViide>
									</avaldamismarge>
									<joustumine>2003-07-01</joustumine>
								</muutmismarge>
							</loige>
							<loige id="para110lg2">
								<loigeNr>2</loigeNr>
								<kuvatavNr><![CDATA[(2)]]></kuvatavNr>
								<muutmismarge>
									<tavatekst>Kehtetu - </tavatekst>
									<avaldamismarge>
										<RTosa>RT I</RTosa>
										<RTaasta>2003</RTaasta>
										<RTnr>13</RTnr>
										<RTartikkel>64</RTartikkel>
										<aktViide>252549</aktViide>
									</avaldamismarge>
									<joustumine>2003-07-01</joustumine>
								</muutmismarge>
